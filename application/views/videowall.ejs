<!doctype html>
<html lang="">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Connecting Purdue Students with the Qualcomm Campus Team">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Q at Purdue</title>

    <!-- favicon -->
    <link rel="shortcut icon" href="/images/favicon.ico">

    <!-- Android Icon-->
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="icon" sizes="196x196" href="/images/touch/chrome-touch-icon-196x196.png">

    <!-- iOS Icon-->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="apple-mobile-web-app-title" content="Q at Purdue">

    <!-- Windows Icon-->
    <meta name="msapplication-TileImage" content="/images/touch/ms-touch-icon-144x144-precomposed.png">
    <meta name="msapplication-TileColor" content="#3372DF">

    <link rel="stylesheet" href="/styles/bootstrap.min.css">
    <link rel="stylesheet" href="/styles/font-awesome.min.css">
    <link rel="stylesheet" href="/styles/main.css">
    <link rel="stylesheet" href="/styles/videowall.css">

    <script src="/scripts/jquery.min.js"></script>
    <script src="/scripts/bootstrap.min.js"></script>
    <script src="/scripts/main.js"></script>
  </head>
  <body>
    <div class="border-container hide-mobile">
    <div class="header">
        <div class="container-fluid">
            <div class="row" style="position: relative">
                <div class="col-xs-5 col-xs-offset-1">
                    <img class="img-responsive" src="/images/logo.png">
                </div>
                <div class="col-xs-4 col-xs-offset-1 title-text">
                    <em>Trivia Leaders</em>
                </div>
            </div>
        </div>
    </div>
    <div class="display-top">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xs-10 col-xs-offset-1">
                    <table class="table leaderboard-table">
                        <thead>
                            <tr>
                                <td>Rank</td>
                                <td>Name</td>
                                <td>Points</td>
                            </tr>
                        </thead>
                        <tbody id="table_body" class="table-hover">
                            <tr>
                                <td>1</td>
                                <td>Levi Starrett</td>
                                <td>10</td>
                            </tr>
                            <tr>
                                <td>2T</td>
                                <td>Clyde Byrd III</td>
                                <td>8</td>
                            </tr>
                            <tr>
                                <td>2T</td>
                                <td>Scott Opell</td>
                                <td>8</td>
                            </tr>
                            <tr>
                                <td>3</td>
                                <td>Andres Bravo</td>
                                <td>7</td>
                            </tr>
                            <tr>
                                <td>4</td>
                                <td>Jake Stevens</td>
                                <td>5</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="display-bottom">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xs-10 col-xs-offset-1">
                    <div id="info-carousel" class="carousel slide" data-ride="carousel">
                        <!-- Wrapper for slides -->
                        <div class="carousel-inner">
                            <div class="item active">
                                <img class="carousel-img" src="/images/logo.png">
                            </div>
                            <div class="item">
                                <img class="carousel-img" src="/images/header-bg.png">
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
  </body>
</html>
